{
    "name": "fusionsuite/backend",
    "description": "It's the backend part of FusionSuite project",
    "type": "project",
    "homepage": "https://fusionsuite.org/",
    "license": [
        "AGPL-3.0-or-later"
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "slim/slim": "4.*",
        "slim/psr7": "1.*",
        "robmorgan/phinx": "0.15.*",
        "illuminate/database": "10.*",
        "tuupola/slim-jwt-auth": "3.*",
        "tuupola/base62": "2.*",
        "spatie/array-to-xml": "3.2.*",
        "hoa/ruler": "^2.17.05.16",
        "illuminate/events": "10.*",
        "rakit/validation": "^1.4",
        "adhocore/cli": "1.6.*",
        "genealabs/laravel-pivot-events": "10.*",
        "symfony/error-handler": "6.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "ActionScripts\\": "ActionScripts"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "1.*",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "10.*",
        "bmitch/churn-php": "^1.7"
    }
}
